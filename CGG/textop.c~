



#include <graphics.h>
#include<stdlib.h>





 
main()
{
   int gd = DETECT, gm, x = 25, y = 25, font = 0;
 
   initgraph(&gd,&gm,NULL);
 
   for (font = 0; font <= 10; font++)
   {
      settextstyle(1,1, 1);
      outtextxy(x, y, "Text with different fonts");
      y = y + 25;
   }
 
   getch();
   closegraph();
   return 0;
}


